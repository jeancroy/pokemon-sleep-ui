{
  "name": "@pokemon-sleep-ui/next-config",
  "packageManager": "yarn@4.1.0",
  "version": "1.0.0",
  "author": "RaenonX",
  "contributors": [
    {
      "name": "John Mitchell-Grant",
      "email": "jgrant_dev@icloud.com"
    }
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "!dist/dts"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^20.10",
    "npm": "^9.5.1"
  },
  "scripts": {
    "dev": "rimraf dist && rollup --config --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config --compact",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-swc": "0.3.0",
    "@rollup/plugin-typescript": "11.1.5",
    "@swc/core": "1.3.102",
    "@types/node": "20.10.6",
    "cross-env": "7.0.3",
    "rimraf": "5.0.5",
    "rollup": "3.28.1",
    "rollup-plugin-dts": "6.0.2",
    "type-fest": "4.9.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  }
}
